h1{
	color:#7DB3FD ;
	font-family:Courier;
}
input{
	border:1px solid #7DB3FD;
	border-radius:10px;
	padding:8px;
	margin:5px;
}
body {
	text-align:center;
	background-color:#D6EAF8;
	margin-top:50px;					

}
.centersignup{
	margin:auto;
	width:40%;
	border:2px solid #C0C0C0;
	border-radius:10px;
}
a{
	text-decoration:none;
	color:#28B463;
	font-family:cursive;
}
.button{
	cursor:pointer;
	padding:5px;
	margin:3px 6px;
	background-color:#F4D03F;
	color:#283747;
	text-align:center;
	font-size:20px;
	border-radius:10%;
}
sup{
	color:#2503FC;
}
div.center{
	margin:auto;
	width:40%;
	border:2px solid #C0C0C0;
	border-radius:10px;
}
.list ul{
	list-style-type:none;
}
li a{
	font-size:18px;
	color:#7DB3FD;
	background-color: #D6EAF8;
	padding: 10px;
	margin-left: -4px;
}
ul li{
	display: inline-block;
}
ul ul{ /* from here next 2 steps are dropdown button*/
	display:none;
	position:absolute;
}
ul li:hover ul{
	display:block;
	overflow:auto;
}
li a:hover{
	color:white;
	background-color:purple;
}
iframe{
	margin-top:10px;
}
